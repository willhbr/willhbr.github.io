---
---
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="/feed.xsl" type="text/xsl"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ site.feeds.rss_url | absolute_url }}" rel="self" type="application/rss+xml" />
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.posts limit:20 %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <pubDate>{{ post.date | smart_date | date_to_rfc822 }}</pubDate>
        <link>{{ post.url | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ post.url | prepend: site.url }}</guid>
        <description>
          {%- capture content -%}{% include feed_post.html %}{%- endcapture -%}
          {%- if content contains "]]>" -%}
            {{ content | strip_highlighting | xml_escape }}
          {%- else -%}
            <![CDATA[{{ content | strip_highlighting }}]]>
          {%- endif -%}
        </description>
      </item>
    {% endfor %}
  </channel>
</rss>
